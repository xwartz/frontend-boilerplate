{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "This is just my personal boilerplate.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src *.js",
    "dev": "node -r babel-register dev-server.js",
    "log": "pm2 logs",
    "stop": "pm2 stop eco.json && pm2 delete eco.json",
    "start": "npm run stop && pm2 start eco.json && npm run log",
    "restart": "npm run stop && npm run start",
    "build": "NODE_ENV=production node -r babel-register ./node_modules/.bin/webpack --config webpack.config.pro.js --progress --profile --colors"
  },
  "pre-commit": ["lint"],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/xwartz/frontend-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "eslint",
    "boilerplate"
  ],
  "author": "xwartz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xwartz/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/xwartz/frontend-boilerplate#readme",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "pm2": "^1.1.3",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.5.2",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "file-loader": "^0.8.5",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-runtime": "^5.8.0",
    "eslint": "^2.12.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.0",
    "redux-thunk": "^2.0.1"
  }
}
